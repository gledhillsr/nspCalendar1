<?xml version="1.0" encoding="ISO-8859-1" ?>

<web-app xmlns="http://java.sun.com/xml/ns/j2ee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
    version="2.4">

    <display-name>NSPOnline Scheduling</display-name>
    <description>
        National Ski Patrol Online Scheduling Software
    </description>

    <servlet>
        <servlet-name>StatusServlet</servlet-name>
        <servlet-class>org.nsponline.calendar.Status</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MemberLoginServlet</servlet-name>
        <servlet-class>org.nsponline.calendar.MemberLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginHelpServlet</servlet-name>
        <servlet-class>org.nsponline.calendar.LoginHelp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MonthCalendarServlet</servlet-name>
        <servlet-class>org.nsponline.calendar.MonthCalendar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DayShiftsServlet</servlet-name>
        <servlet-class>org.nsponline.calendar.DayShifts</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangeShiftServlet</servlet-name>
        <servlet-class>org.nsponline.calendar.ChangeShift</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessChangesServlet</servlet-name>
        <servlet-class>org.nsponline.calendar.ProcessChanges</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DirectorsServlet</servlet-name>
        <servlet-class>org.nsponline.calendar.Directors</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateInfoServlet</servlet-name>
        <servlet-class>org.nsponline.calendar.UpdateInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListAssignmentsServlet</servlet-name>
        <servlet-class>org.nsponline.calendar.ListAssignments</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CustomizedListServlet</servlet-name>
        <servlet-class>org.nsponline.calendar.CustomizedList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CustomizedList2Servlet</servlet-name>
        <servlet-class>org.nsponline.calendar.CustomizedList2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubListServlet</servlet-name>
        <servlet-class>org.nsponline.calendar.SubList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PreferencesServlet</servlet-name>
        <servlet-class>org.nsponline.calendar.Preferences</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditShiftsServlet</servlet-name>
        <servlet-class>org.nsponline.calendar.EditShifts</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PurgeAssignmentsServlet</servlet-name>
        <servlet-class>org.nsponline.calendar.PurgeAssignments</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmailFormServlet</servlet-name>
        <servlet-class>org.nsponline.calendar.EmailForm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MemberListServlet</servlet-name>
        <servlet-class>org.nsponline.calendar.MemberList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MemberLogoutServlet</servlet-name>
        <servlet-class>org.nsponline.calendar.MemberLogout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>User</servlet-name>
        <servlet-class>org.nsponline.calendar.rest.User</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserAssignments</servlet-name>
        <servlet-class>org.nsponline.calendar.rest.UserAssignments</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>org.nsponline.calendar.rest.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>org.nsponline.calendar.rest.Login</servlet-class>
    </servlet>

<!--    define & map URL patterns -->
    <servlet-mapping>
        <servlet-name>StatusServlet</servlet-name>
        <url-pattern>/Status</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MemberLoginServlet</servlet-name>
        <url-pattern>/MemberLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MemberLogoutServlet</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>User</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserAssignments</servlet-name>
        <url-pattern>/user/assignments</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginHelpServlet</servlet-name>
        <url-pattern>/LoginHelp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MonthCalendarServlet</servlet-name>
        <url-pattern>/MonthCalendar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DayShiftsServlet</servlet-name>
        <url-pattern>/DayShifts</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangeShiftServlet</servlet-name>
        <url-pattern>/ChangeShift</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessChangesServlet</servlet-name>
        <url-pattern>/ProcessChanges</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DirectorsServlet</servlet-name>
        <url-pattern>/Directors</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateInfoServlet</servlet-name>
        <url-pattern>/UpdateInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListAssignmentsServlet</servlet-name>
        <url-pattern>/ListAssignments</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CustomizedListServlet</servlet-name>
        <url-pattern>/CustomizedList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CustomizedList2Servlet</servlet-name>
        <url-pattern>/CustomizedList2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubListServlet</servlet-name>
        <url-pattern>/SubList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PreferencesServlet</servlet-name>
        <url-pattern>/Preferences</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditShiftsServlet</servlet-name>
        <url-pattern>/EditShifts</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PurgeAssignmentsServlet</servlet-name>
        <url-pattern>/PurgeAssignments</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmailFormServlet</servlet-name>
        <url-pattern>/EmailForm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MemberListServlet</servlet-name>
        <url-pattern>/MemberList</url-pattern>
    </servlet-mapping>

</web-app> 
